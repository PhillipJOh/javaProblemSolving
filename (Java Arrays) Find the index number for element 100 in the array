// Write a Java program to find the index of an array element. Find the index number for element 100 in the array

import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	   int[] arr = {5,20,40,50,80,100,120,160,200};
	   
	   for(int i=0; i<arr.length; i++){
	       if(arr[i]==100){
	           System.out.print(i);
	           break;
	       }
	   }
	}
}
