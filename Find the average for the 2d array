// Find the average for the 2D array

public class Main
{
	public static void main(String[] args) {
		int[][] arr = {{22,14,999},{81},{10,36}};
	    double total = 0;
		double count = 0;
		
		for(int i = 0; i<arr.length; i++){
		    for(int j = 0; j<arr[i].length; j++){
		       total = total + arr[i][j];
		       count++;
		    }
		}
		double average = 0;
		average = total/count;
		System.out.print(average);
	}
}
