// Program to find the second largest element in an array. 

public class Main
{
	public static void main(String[] args) {
		int[] arr = {1,2,5,8,9,3,4};
		
		int max = arr[0];
		int max2 = arr[0];
		
		for(int i=0; i<arr.length; i++){
		    if(max<arr[i]){
		        max=arr[i];
		    }
		}
		for(int i=0; i<arr.length; i++){
		    if(max2<arr[i]&&arr[i]!=max){
		        max2=arr[i];
		    }
		}
		System.out.println(max2);
	}
}
