// Iterate through your int 2d array and double the elements

public class Main
{
	public static void main(String[] args) {
		int[][] arr = {{20,6},{2,18,40,24},{8,120,16}};
		
	    for(int i = 0; i<arr.length; i++){
	        for(int j = 0; j < arr[i].length; j++){
	            arr[i][j]=arr[i][j]*2;
	        }
	    }
	    for(int i = 0; i<arr.length; i++){
	        for(int j = 0; j < arr[i].length; j++){
	            System.out.println(arr[i][j]);
	        }
	    }
	}
}
