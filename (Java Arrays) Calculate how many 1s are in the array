// Write a Java program to test if an array contains a specific value. Calculate how many 1s are in the array

import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	   int[] arr = {10,9,8,4,3,2,1,3,1,2,1};
	   int result = 0;
	   
	   for(int i=0; i<arr.length; i++){
	       if(arr[i]==1){
	           result = result + 1;
	       }
	   }
	   System.out.println(result);
	}
}
